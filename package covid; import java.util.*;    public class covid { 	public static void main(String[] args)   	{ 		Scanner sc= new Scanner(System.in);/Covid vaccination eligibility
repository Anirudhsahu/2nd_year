package covid;
import java.util.*;  

public class covid {
	public static void main(String[] args)  
	{
		Scanner sc= new Scanner(System.in);    //System.in is a standard input stream  
		System.out.println("Enter age to find whether eligilbe or not for vaccination - ");  
		int a= sc.nextInt();
		System.out.println("Enter aadhar card number:- ");
		int b= sc.nextInt();
		if(a>=18)
		{
			if(b != 0)
			{
				System.out.println("The candidate is eligible");
				
			}
			
			
		}
		else
		{
				System.out.println("The candidate is ineligible for the vaccination");
		}
	}
}

