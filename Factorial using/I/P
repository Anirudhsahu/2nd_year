package fact;
import java.util.*;  

public class fact {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int f=1,i;
		System.out.println("Enter the value to find its factorial:- ");
		int a = sc.nextInt();
		i=1;
		while(i<=a)
		{
			f=f*i;
			i++;		
		}
		System.out.println("The factorial of the given value is ="+f);
	}

}
